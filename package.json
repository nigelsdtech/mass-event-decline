{
  "name": "mass-event-decline",
  "version": "1.0.0",
  "description": "Allows you to decline all events (and leave a decline note) in your google calendar between two specified dates. Perfect for declining all invitations whilst you're on leave. There is also the option of setting certain exceptions for events you don't want to delete, or deleting events that you don't want to keep.",
  "main": "index.js",
  "scripts": {
    "start": "sh scripts/start.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "out of office",
    "decline",
    "event",
    "calendar"
  ],
  "author": "Nigel Stephen D'Souza",
  "license": "ISC",
  "dependencies": {
    "calendar-model": "git://github.com/nigelsdtech/calendar-model.git#34df8f25cf904a68f91ebf1124efd3b63a1bbdfd",
    "config": "1.x",
    "dateformat": "1.0.12",
    "internet-timestamp": "0.x",
    "log4js": "0.x",
    "semver": "4.3.6"
  },
  "config": {
    "appName": "mass-event-decline",
    "googleAuthScopes": "https://www.googleapis.com/auth/calendar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nigelsdtech/mass-event-decline.git"
  },
  "bugs": {
    "url": "https://github.com/nigelsdtech/mass-event-decline/issues"
  },
  "homepage": "https://github.com/nigelsdtech/mass-event-decline#readme",
  "devDependencies": {}
}
